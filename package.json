{
  "name": "artsy-blog",
  "description": "Artsy Engineering Blog for all enginers over the world",
  "version": "1.0.0",
  "author": "Artsy",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "start": "npm run develop",
    "format": "prettier --write \"{,!(node_modules|public|coverage|.cache)/**/}*.{js,jsx,ts,tsx,json}\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "gatsby",
    "typescript"
  ],
  "dependencies": {
    "@fec/gatsby-plugin-advanced-feed": "^1.0.0",
    "babel-plugin-styled-components": "^1.10.6",
    "gatsby": "^2.15.20",
    "gatsby-image": "^2.2.20",
    "gatsby-plugin-lodash": "^3.1.8",
    "gatsby-plugin-lunr": "^1.5.2",
    "gatsby-plugin-manifest": "^2.2.18",
    "gatsby-plugin-offline": "^3.0.8",
    "gatsby-plugin-react-helmet": "^3.1.8",
    "gatsby-plugin-sharp": "^2.2.25",
    "gatsby-plugin-styled-components": "^3.1.8",
    "gatsby-plugin-typescript": "^2.1.9",
    "gatsby-remark-embed-video": "^1.7.1",
    "gatsby-remark-external-links": "0.0.4",
    "gatsby-remark-images": "^3.1.23",
    "gatsby-remark-responsive-iframe": "^2.2.17",
    "gatsby-source-filesystem": "^2.1.26",
    "gatsby-transformer-remark": "^2.6.24",
    "gatsby-transformer-sharp": "^2.2.16",
    "lodash": "^4.17.15",
    "prop-types": "^15.7.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-helmet": "^5.2.1",
    "styled-components": "^4.4.0",
    "typescript": "^3.6.3"
  },
  "devDependencies": {
    "@types/node": "^12.7.4",
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.9.0",
    "@types/react-helmet": "^5.0.10",
    "husky": "^3.0.5",
    "lint-staged": ">=9",
    "prettier": "1.18.2",
    "tslint": "^5.20.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^4.1.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/frnki/gatsby-typescript-starter-blog"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json}": [
      "prettier --write",
      "git add"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": false,
    "trailingComma": "es5",
    "bracketSpacing": true
  },
  "resolutions": {
    "version": "14.5.6"
  }
}
